<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:include schemaLocation="IFSData.xsd"/>
	<xs:element name="ImportCardRejectFile">
		<xs:annotation>
			<xs:documentation>Indicates that Affina has rejected an import card file request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="FileName" type="FileNameType">
					<xs:annotation>
						<xs:documentation>Name of the file that was transferred</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FileRejection" type="RejectionType">
					<xs:annotation>
						<xs:documentation>Identifies why the file was rejected</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
